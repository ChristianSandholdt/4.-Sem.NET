<Window x:Class="Lektion8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lektion8"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lbCarView" HorizontalAlignment="Left" Height="259" Margin="32,44,0,0" VerticalAlignment="Top" Width="263" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="btnAddCar" Content="Tilføj bil" HorizontalAlignment="Center" Height="55" Margin="0,44,0,0" VerticalAlignment="Top" Width="166" Click="Button_Click"/>
        <ComboBox x:Name="cBoxEjere" Margin="532,44,90,335" SelectionChanged="ComboBox_SelectionChanged"/>

    </Grid>
</Window>
