<Window x:Class="Opgave6_6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Opgave6_6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <Button x:Name="vestKnap" DockPanel.Dock="Left" Click="vestKnap_Click">Vest</Button>
            <Button x:Name="nordKnap" DockPanel.Dock="Top" Click="nordKnap_Click">Nord</Button>
            <Button x:Name="østKnap" DockPanel.Dock="Right" Click="østKnap_Click">Øst</Button>
            <Button x:Name="sydKnap" DockPanel.Dock="Bottom" Click="sydKnap_Click">Syd</Button>
            <TextBox Name="textbox" TextAlignment="Center"></TextBox>
        </DockPanel>

    </Grid>
</Window>
